{"ast":null,"code":"import { computed, ref } from 'vue';\nimport { useRouter, useRoute } from 'vue-router';\nimport { useStore } from 'vuex';\nexport default {\n  name: 'BottomNavigation',\n  setup() {\n    const router = useRouter();\n    const route = useRoute();\n    const store = useStore();\n    const loading = ref(false);\n    const menuItems = [{\n      path: '/todos',\n      title: '투두리스트',\n      icon: 'fnb_todolist'\n    }, {\n      path: '/calendar',\n      title: '캘린더',\n      icon: 'fnb_calendar'\n    }, {\n      path: '/notices',\n      title: '공지사항',\n      icon: 'fnb_notice'\n    }, {\n      path: '/diary',\n      title: '하루일기',\n      icon: 'fnb_daily'\n    }, {\n      path: '/mypage',\n      title: '마이페이지',\n      icon: 'fnb_mypage'\n    }];\n    const isAuthenticated = computed(() => store.getters['auth/isAuthenticated']);\n    const isStandalone = computed(() => window.matchMedia('(display-mode: standalone)').matches);\n    const navigateTo = async path => {\n      if (!isAuthenticated.value) {\n        router.push('/login');\n        return;\n      }\n      if (route.path === path) {\n        return; // 이미 해당 페이지에 있으면 이동하지 않음\n      }\n      loading.value = true;\n      try {\n        await router.push(path);\n      } catch (error) {\n        console.error('Navigation error:', error);\n      } finally {\n        loading.value = false;\n      }\n    };\n    const isActive = path => {\n      return route.path === path;\n    };\n    return {\n      menuItems,\n      navigateTo,\n      isActive,\n      isAuthenticated,\n      isStandalone,\n      loading\n    };\n  }\n};","map":{"version":3,"names":["computed","ref","useRouter","useRoute","useStore","name","setup","router","route","store","loading","menuItems","path","title","icon","isAuthenticated","getters","isStandalone","window","matchMedia","matches","navigateTo","value","push","error","console","isActive"],"sources":["D:\\03.기타\\todolist\\frontend\\src\\components\\BottomNavigation.vue"],"sourcesContent":["<template>\r\n  <nav class=\"bottom-navigation\" :class=\"{ 'standalone': isStandalone }\">\r\n    <ul class=\"fnb_list\">\r\n      <li v-for=\"item in menuItems\" :key=\"item.path\">\r\n        <button \r\n          type=\"button\" \r\n          @click=\"navigateTo(item.path)\" \r\n          class=\"btn_fnb\"\r\n          :class=\"{ active: isActive(item.path) }\"\r\n          :disabled=\"loading\"\r\n        >\r\n          <i :class=\"['fnb_icon', item.icon]\"></i>\r\n          <span class=\"fnb_tit\">{{ item.title }}</span>\r\n        </button>\r\n      </li>\r\n    </ul>\r\n  </nav>\r\n</template>\r\n\r\n<script>\r\nimport { computed, ref } from 'vue'\r\nimport { useRouter, useRoute } from 'vue-router'\r\nimport { useStore } from 'vuex'\r\n\r\nexport default {\r\n  name: 'BottomNavigation',\r\n  setup() {\r\n    const router = useRouter()\r\n    const route = useRoute()\r\n    const store = useStore()\r\n    const loading = ref(false)\r\n\r\n    const menuItems = [\r\n      {\r\n        path: '/todos',\r\n        title: '투두리스트',\r\n        icon: 'fnb_todolist'\r\n      },\r\n      {\r\n        path: '/calendar',\r\n        title: '캘린더',\r\n        icon: 'fnb_calendar'\r\n      },\r\n      {\r\n        path: '/notices',\r\n        title: '공지사항',\r\n        icon: 'fnb_notice'\r\n      },\r\n      {\r\n        path: '/diary',\r\n        title: '하루일기',\r\n        icon: 'fnb_daily'\r\n      },\r\n      {\r\n        path: '/mypage',\r\n        title: '마이페이지',\r\n        icon: 'fnb_mypage'\r\n      }\r\n    ]\r\n\r\n    const isAuthenticated = computed(() => store.getters['auth/isAuthenticated'])\r\n    const isStandalone = computed(() => window.matchMedia('(display-mode: standalone)').matches)\r\n\r\n    const navigateTo = async (path) => {\r\n      if (!isAuthenticated.value) {\r\n        router.push('/login')\r\n        return\r\n      }\r\n      \r\n      if (route.path === path) {\r\n        return // 이미 해당 페이지에 있으면 이동하지 않음\r\n      }\r\n      \r\n      loading.value = true\r\n      try {\r\n        await router.push(path)\r\n      } catch (error) {\r\n        console.error('Navigation error:', error)\r\n      } finally {\r\n        loading.value = false\r\n      }\r\n    }\r\n\r\n    const isActive = (path) => {\r\n      return route.path === path\r\n    }\r\n\r\n    return {\r\n      menuItems,\r\n      navigateTo,\r\n      isActive,\r\n      isAuthenticated,\r\n      isStandalone,\r\n      loading\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n/* BottomNavigation 컴포넌트 특별 스타일만 유지 */\r\n.fnb_notice {\r\n  background-image: url('@/assets/images/common/fnb_notice.svg');\r\n}\r\n</style>\r\n"],"mappings":"AAoBA,SAASA,QAAQ,EAAEC,GAAE,QAAS,KAAI;AAClC,SAASC,SAAS,EAAEC,QAAO,QAAS,YAAW;AAC/C,SAASC,QAAO,QAAS,MAAK;AAE9B,eAAe;EACbC,IAAI,EAAE,kBAAkB;EACxBC,KAAKA,CAAA,EAAG;IACN,MAAMC,MAAK,GAAIL,SAAS,CAAC;IACzB,MAAMM,KAAI,GAAIL,QAAQ,CAAC;IACvB,MAAMM,KAAI,GAAIL,QAAQ,CAAC;IACvB,MAAMM,OAAM,GAAIT,GAAG,CAAC,KAAK;IAEzB,MAAMU,SAAQ,GAAI,CAChB;MACEC,IAAI,EAAE,QAAQ;MACdC,KAAK,EAAE,OAAO;MACdC,IAAI,EAAE;IACR,CAAC,EACD;MACEF,IAAI,EAAE,WAAW;MACjBC,KAAK,EAAE,KAAK;MACZC,IAAI,EAAE;IACR,CAAC,EACD;MACEF,IAAI,EAAE,UAAU;MAChBC,KAAK,EAAE,MAAM;MACbC,IAAI,EAAE;IACR,CAAC,EACD;MACEF,IAAI,EAAE,QAAQ;MACdC,KAAK,EAAE,MAAM;MACbC,IAAI,EAAE;IACR,CAAC,EACD;MACEF,IAAI,EAAE,SAAS;MACfC,KAAK,EAAE,OAAO;MACdC,IAAI,EAAE;IACR,EACF;IAEA,MAAMC,eAAc,GAAIf,QAAQ,CAAC,MAAMS,KAAK,CAACO,OAAO,CAAC,sBAAsB,CAAC;IAC5E,MAAMC,YAAW,GAAIjB,QAAQ,CAAC,MAAMkB,MAAM,CAACC,UAAU,CAAC,4BAA4B,CAAC,CAACC,OAAO;IAE3F,MAAMC,UAAS,GAAI,MAAOT,IAAI,IAAK;MACjC,IAAI,CAACG,eAAe,CAACO,KAAK,EAAE;QAC1Bf,MAAM,CAACgB,IAAI,CAAC,QAAQ;QACpB;MACF;MAEA,IAAIf,KAAK,CAACI,IAAG,KAAMA,IAAI,EAAE;QACvB,OAAK,CAAE;MACT;MAEAF,OAAO,CAACY,KAAI,GAAI,IAAG;MACnB,IAAI;QACF,MAAMf,MAAM,CAACgB,IAAI,CAACX,IAAI;MACxB,EAAE,OAAOY,KAAK,EAAE;QACdC,OAAO,CAACD,KAAK,CAAC,mBAAmB,EAAEA,KAAK;MAC1C,UAAU;QACRd,OAAO,CAACY,KAAI,GAAI,KAAI;MACtB;IACF;IAEA,MAAMI,QAAO,GAAKd,IAAI,IAAK;MACzB,OAAOJ,KAAK,CAACI,IAAG,KAAMA,IAAG;IAC3B;IAEA,OAAO;MACLD,SAAS;MACTU,UAAU;MACVK,QAAQ;MACRX,eAAe;MACfE,YAAY;MACZP;IACF;EACF;AACF","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}