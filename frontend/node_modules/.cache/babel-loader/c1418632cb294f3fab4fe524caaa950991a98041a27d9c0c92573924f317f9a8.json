{"ast":null,"code":"import { computed, onMounted } from 'vue';\nimport { useStore } from 'vuex';\nexport default {\n  name: 'App',\n  setup() {\n    const store = useStore();\n    const appTheme = computed(() => {\n      const user = store.state.auth.user;\n      if (!user) return 'lightmode style_simple palette_simple';\n      return `${user.modePreference || 'lightmode'} ${user.themePreference || 'style_simple'} ${user.colorPreference || 'palette_simple'}`;\n    });\n\n    // 페이지 로드 시 인증 상태 복원\n    onMounted(async () => {\n      const token = store.state.auth.token;\n      if (token && !store.state.auth.user) {\n        try {\n          await store.dispatch('auth/getCurrentUser');\n        } catch (error) {\n          console.error('Failed to restore authentication state:', error);\n          // 토큰이 유효하지 않으면 로그아웃\n          store.dispatch('auth/logout');\n        }\n      }\n    });\n    return {\n      appTheme\n    };\n  }\n};","map":{"version":3,"names":["computed","onMounted","useStore","name","setup","store","appTheme","user","state","auth","modePreference","themePreference","colorPreference","token","dispatch","error","console"],"sources":["D:\\03.기타\\todolist\\frontend\\src\\App.vue"],"sourcesContent":["<template>\r\n  <div id=\"app\" :class=\"appTheme\">\r\n    <router-view />\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { computed, onMounted } from 'vue'\r\nimport { useStore } from 'vuex'\r\n\r\nexport default {\r\n  name: 'App',\r\n  setup() {\r\n    const store = useStore()\r\n    \r\n    const appTheme = computed(() => {\r\n      const user = store.state.auth.user\r\n      if (!user) return 'lightmode style_simple palette_simple'\r\n      \r\n      return `${user.modePreference || 'lightmode'} ${user.themePreference || 'style_simple'} ${user.colorPreference || 'palette_simple'}`\r\n    })\r\n    \r\n    // 페이지 로드 시 인증 상태 복원\r\n    onMounted(async () => {\r\n      const token = store.state.auth.token\r\n      if (token && !store.state.auth.user) {\r\n        try {\r\n          await store.dispatch('auth/getCurrentUser')\r\n        } catch (error) {\r\n          console.error('Failed to restore authentication state:', error)\r\n          // 토큰이 유효하지 않으면 로그아웃\r\n          store.dispatch('auth/logout')\r\n        }\r\n      }\r\n    })\r\n    \r\n    return {\r\n      appTheme\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style>\r\n#app {\r\n  font-family: 'Noto Sans KR', Avenir, Helvetica, Arial, sans-serif;\r\n  -webkit-font-smoothing: antialiased;\r\n  -moz-osx-font-smoothing: grayscale;\r\n  min-height: 100vh;\r\n  transition: all 0.3s ease;\r\n}\r\n\r\n/* 테마별 스타일 */\r\n.lightmode {\r\n  background-color: #ffffff;\r\n  color: #333333;\r\n}\r\n\r\n.darkmode {\r\n  background-color: #1a1a1a;\r\n  color: #ffffff;\r\n}\r\n\r\n/* 반응형 디자인 */\r\n@media (max-width: 768px) {\r\n  #app {\r\n    padding: 10px;\r\n  }\r\n}\r\n</style>\r\n"],"mappings":"AAOA,SAASA,QAAQ,EAAEC,SAAQ,QAAS,KAAI;AACxC,SAASC,QAAO,QAAS,MAAK;AAE9B,eAAe;EACbC,IAAI,EAAE,KAAK;EACXC,KAAKA,CAAA,EAAG;IACN,MAAMC,KAAI,GAAIH,QAAQ,CAAC;IAEvB,MAAMI,QAAO,GAAIN,QAAQ,CAAC,MAAM;MAC9B,MAAMO,IAAG,GAAIF,KAAK,CAACG,KAAK,CAACC,IAAI,CAACF,IAAG;MACjC,IAAI,CAACA,IAAI,EAAE,OAAO,uCAAsC;MAExD,OAAO,GAAGA,IAAI,CAACG,cAAa,IAAK,WAAW,IAAIH,IAAI,CAACI,eAAc,IAAK,cAAc,IAAIJ,IAAI,CAACK,eAAc,IAAK,gBAAgB,EAAC;IACrI,CAAC;;IAED;IACAX,SAAS,CAAC,YAAY;MACpB,MAAMY,KAAI,GAAIR,KAAK,CAACG,KAAK,CAACC,IAAI,CAACI,KAAI;MACnC,IAAIA,KAAI,IAAK,CAACR,KAAK,CAACG,KAAK,CAACC,IAAI,CAACF,IAAI,EAAE;QACnC,IAAI;UACF,MAAMF,KAAK,CAACS,QAAQ,CAAC,qBAAqB;QAC5C,EAAE,OAAOC,KAAK,EAAE;UACdC,OAAO,CAACD,KAAK,CAAC,yCAAyC,EAAEA,KAAK;UAC9D;UACAV,KAAK,CAACS,QAAQ,CAAC,aAAa;QAC9B;MACF;IACF,CAAC;IAED,OAAO;MACLR;IACF;EACF;AACF","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}